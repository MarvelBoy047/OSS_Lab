{
  "name": "oss-ui",
  "version": "1.0.0",
  "license": "MIT",
  "author": "MarvelBOy",
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev",
    "electron-dev": "concurrently --kill-others \"npm run dev\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development ELECTRON_DISABLE_SECURITY_WARNINGS=true electron .\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format:write": "prettier . --write",
    "electron-build": "npm run build && electron-builder",
    "build-win": "npm run build && electron-builder --windows",
    "build-all": "npm run build && electron-builder -mwl"
  },
  "build": {
    "appId": "com.osslab.desktop",
    "productName": "OSSLAB",
    "directories": {
      "out": "dist"
    },
    "files": [
      "electron/",
      "out/",
      "node_modules/",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "electron/assets/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "electron/assets/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "electron/assets/icon.png"
    }
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@iarna/toml": "^2.2.5",
    "@icons-pack/react-simple-icons": "^12.3.0",
    "@langchain/anthropic": "^0.3.24",
    "@langchain/community": "^0.3.49",
    "@langchain/core": "^0.3.66",
    "@langchain/google-genai": "^0.2.15",
    "@langchain/groq": "^0.2.3",
    "@langchain/ollama": "^0.2.3",
    "@langchain/openai": "^0.6.2",
    "@langchain/textsplitters": "^0.1.0",
    "axios": "^1.8.3",
    "clsx": "^2.1.0",
    "compute-cosine-similarity": "^1.1.0",
    "compute-dot": "^1.1.0",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "html-to-text": "^9.0.5",
    "jspdf": "^3.0.1",
    "langchain": "^0.3.30",
    "lucide-react": "^0.363.0",
    "mammoth": "^1.9.1",
    "markdown-to-jsx": "^7.7.2",
    "next": "^15.2.2",
    "next-themes": "^0.3.0",
    "pdf-parse": "^1.1.1",
    "pptxgenjs": "^4.0.1",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.6",
    "react-text-to-speech": "^0.14.5",
    "react-textarea-autosize": "^8.5.3",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.2.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5",
    "yet-another-react-lightbox": "^3.17.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "^10.0.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.1.0",
    "electron-builder": "^26.0.12",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "kill-port": "^2.0.1",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "wait-on": "^8.0.4"
  }
}
